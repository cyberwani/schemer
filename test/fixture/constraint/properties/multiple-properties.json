{
    "schema": {
        "properties": {
            "foo": {
                "type": "string"
            },
            "bar": {
                "type": "object"
            },
            "baz": {
                "type": "null"
            }
        }
    },

    "tests": {
        "valid": {
            "value": {
                "foo": "qux",
                "bar": {}
            },
            "expected": []
        },

        "invalid": {
            "value": {
                "foo": null,
                "bar": "doom",
                "baz": null
            },
            "expected": [
                "Validation failed for value at '/foo': The value must be of type 'string'.",
                "Validation failed for value at '/bar': The value must be of type 'object'."
            ]
        }
    }
}

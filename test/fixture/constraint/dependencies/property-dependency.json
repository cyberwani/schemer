{
    "schema": {
        "dependencies": {
            "foo": ["bar", "baz"]
        }
    },

    "tests": {
        "valid - dependant": {
            "value": {"foo": "baz", "bar": null, "baz": null},
            "expected": []
        },

        "valid - not dependant": {
            "value": {"qux": 111},
            "expected": []
        },

        "valid non-object": {
            "value": "foo",
            "expected": []
        },

        "invalid": {
            "value": {"foo": "baz", "bar": null},
            "expected": [
                "Validation failed for value at document root: The property \"baz\" is required."
            ]
        }
    }
}
